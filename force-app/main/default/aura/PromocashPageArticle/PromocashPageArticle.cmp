<aura:component implements="forceCommunity:layout" description="Page Article Promocash" access="global">
    <aura:attribute name="header" type="Aura.Component[]" required="false"></aura:attribute>
    <aura:attribute name="content" type="Aura.Component[]" required="false"></aura:attribute>
    <aura:attribute name="bottom" type="Aura.Component[]" required="false"></aura:attribute>
    <aura:attribute name="footer" type="Aura.Component[]" required="false"></aura:attribute>
    <div class="detailArticle">
        <div class="container">
            <div class="header slds-m-bottom_xx-large">
                {!v.header}
            </div>
            <div class="contentPanel slds-m-bottom_xx-large">
                <div class="content"> 
                    {!v.content}
                </div> 
                <div class="bottom"> 
                    {!v.bottom}
                </div>                 
            </div>
            <div class="footer">
                {!v.footer}
            </div>
        </div>
    </div>
    
</aura:component>